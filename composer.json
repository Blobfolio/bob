{
    "name": "blobfolio\/bob",
    "description": "A simple release builder framework for Blobfolio projects.",
    "version": "1.0.0",
    "type": "library",
    "homepage": "https:\/\/github.com\/Blobfolio\/bob",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Josh Stoik",
            "email": "josh@blobfolio.com",
            "homepage": "https:\/\/blobfolio.com"
        }
    ],
    "require": {
        "php": ">= 7.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-readline": "*",
        "ext-zip": "*",
        "blobfolio\/blob-common": "dev-7.0_Next",
        "blobfolio\/blob-mimes": "dev-7.0_Next",
        "blobfolio\/blob-phone": "dev-7.0_Next",
        "blobfolio\/blob-domain": "dev-7.0_Next"
    },
    "config": {
        "vendor-dir": "lib\/vendor",
        "preferred-install": "dist"
    },
    "scripts": {
        "post-update-cmd": "blobfolio\\bob\\himself::install",
        "post-install-cmd": "blobfolio\\bob\\himself::install"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "blobfolio\\bob\\": "lib\/blobfolio\/bob"
        },
        "files": []
    }
}
